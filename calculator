#include <iostream>
using namespace std;
int main(){
    
    
    int q,w;
    
    q=0;
    w=0;
    
    char s;
    
    
    while(true){
        cin>>w>>q>>s;
    switch(s){
        
        
        case '*':
        cout<<w*q;
        break;
        
        case '/':
        cout<<w/q;
        break;
        
        case '-':
        cout<<w-q;
        break;
        
        case '+':
        cout<<w+q;
        break;
        
        case '1':
        return 0;
        
        default:
        return 0;
        
    }    
    }
}
